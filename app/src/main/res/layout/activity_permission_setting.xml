<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/layout_common_toolbar" />

    <RelativeLayout
        android:id="@+id/permission_phone_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_len_118px"
        android:foreground="?android:attr/selectableItemBackground"
        android:paddingLeft="@dimen/common_len_30px"
        android:paddingRight="@dimen/common_len_30px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/permission_phone_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />

        <TextView
            android:id="@+id/permission_phone_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/icon_jinru"
            android:text="@string/withdraw_has_set_info_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/permission_storage_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_len_118px"
        android:foreground="?android:attr/selectableItemBackground"
        android:paddingLeft="@dimen/common_len_30px"
        android:paddingRight="@dimen/common_len_30px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/permission_file_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />

        <TextView
            android:id="@+id/permission_storage_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/icon_jinru"
            android:text="@string/withdraw_has_set_info_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/permission_location_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_len_118px"
        android:foreground="?android:attr/selectableItemBackground"
        android:paddingLeft="@dimen/common_len_30px"
        android:paddingRight="@dimen/common_len_30px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/permission_location_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />

        <TextView
            android:id="@+id/permission_location_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/icon_jinru"
            android:text="@string/withdraw_has_set_info_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/permission_camera_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_len_118px"
        android:foreground="?android:attr/selectableItemBackground"
        android:paddingLeft="@dimen/common_len_30px"
        android:paddingRight="@dimen/common_len_30px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/permission_camera_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />

        <TextView
            android:id="@+id/permission_camera_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:drawableRight="@drawable/icon_jinru"
            android:text="@string/withdraw_has_set_info_title"
            android:textColor="@color/color_E7E7E7"
            android:textSize="@dimen/common_text_size_36px" />
    </RelativeLayout>
</LinearLayout>